<p id="notice"><%= notice %></p>

<h1>Listing Waybillorders</h1>

<table>
  <thead>
    <tr>
      <th>Ordernum</th>
      <th>Goodsname</th>
      <th>Number</th>
      <th>Weight</th>
      <th>Volume</th>
      <th>Freightcost</th>
      <th>Goodsprice</th>
      <th>Insurancecost</th>
      <th>Packingcost</th>
      <th>Delivercost</th>
      <th>Acceptcost</th>
      <th>Istransfer</th>
      <th>Transfercost</th>
      <th>Total</th>
      <th>Place</th>
      <th>Destination</th>
      <th>Consignor</th>
      <th>Consignee</th>
      <th>Proxycollection</th>
      <th>Truck</th>
      <th>Company</th>
      <th>User</th>
      <th>Paymentmethod</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @waybillorders.each do |waybillorder| %>
      <tr>
        <td><%= waybillorder.ordernum %></td>
        <td><%= waybillorder.goodsname %></td>
        <td><%= waybillorder.number %></td>
        <td><%= waybillorder.weight %></td>
        <td><%= waybillorder.volume %></td>
        <td><%= waybillorder.freightCost %></td>
        <td><%= waybillorder.goodsPrice %></td>
        <td><%= waybillorder.insuranceCost %></td>
        <td><%= waybillorder.packingCost %></td>
        <td><%= waybillorder.deliverCost %></td>
        <td><%= waybillorder.acceptCost %></td>
        <td><%= waybillorder.isTransfer %></td>
        <td><%= waybillorder.transferCost %></td>
        <td><%= waybillorder.total %></td>
        <td><%= waybillorder.place_id %></td>
        <td><%= waybillorder.destination_id %></td>
        <td><%= waybillorder.consignor_id %></td>
        <td><%= waybillorder.consignee_id %></td>
        <td><%= waybillorder.proxycollection %></td>
        <td><%= waybillorder.truck_id %></td>
        <td><%= waybillorder.company_id %></td>
        <td><%= waybillorder.user_id %></td>
        <td><%= waybillorder.paymentmethod_id %></td>
        <td><%= link_to 'Show', waybillorder %></td>
        <td><%= link_to 'Edit', edit_waybillorder_path(waybillorder) %></td>
        <td><%= link_to 'Destroy', waybillorder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Waybillorder', new_waybillorder_path %>
